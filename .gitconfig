[alias]
    st = status -s -uno
    ci = commit
    br = branch
    fa = fetch --all
    co = checkout
    ls = ls-files
    df = diff
    ds = diff --staged
    ll = log --graph --oneline
    rh = reset --hard
    cleanup = commit -am "Code cleanup"
    amend = commit --amend --reuse-message=HEAD
[core]
    pager = less -F -X
    editor = vim
[url "git@gitlab.com:"]
    insteadOf = gl:
[url "git@github.com:"]
    insteadOf = gh:
